app.js
require("./hbs-helpers")

// require-hbs.js
const hbs = require('hbs')
hbs.registerHelper("name", () =>{
    do stuff here
});

<tab1 />
<tab2 />
<tab3 />

<content1 class="tab-content show" />
<content2 class="tab-content hide" />
<content3 class="tab-content hide" />

<script>
  document.querySelectorAll(".tab-content").forEach((htmlElement) => {
    htmlElement.addEventListener("click", () => {
      if (htmlElement.classList.contains("hide")) {
        htmlElement.classList.remove("hide");
        htmlElement.classList.add("show");
      }

    });
  });
</script>

<script src="/js/tab-logic.js"></script>

